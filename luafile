local clothingIDs = {
     "13085933663", "16027104477", "97760987071166", 
    "97760987071166", "4871915190", -- –ù–æ–≤—ã–µ ID –¥–ª—è —à—Ç–∞–Ω–æ–≤
    "13469431113", "13117322111", "13686174169", -- –°—Ç–∞—Ä—ã–µ ID
    "18698179940", "3716494426", "11849088400",
    "7896098117", "8509230330", "4615028029",
    "7675755130", "6844713210", "126118133870839"
}

print("=== –ù–ê–ß–ê–õ–û –ü–†–û–í–ï–†–ö–ò ===")
local clothingFolder = workspace:FindFirstChild("Clothing")
if not clothingFolder then
    warn("‚ùå –û–®–ò–ë–ö–ê: –ü–∞–ø–∫–∞ Clothing –Ω–µ –Ω–∞–π–¥–µ–Ω–∞ –≤ Workspace!")
    print("–£–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ –≤ Workspace —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ø–∞–ø–∫–∞ —Å –∏–º–µ–Ω–µ–º 'Clothing'")
    return
end
print("‚úî –ü–∞–ø–∫–∞ Clothing –Ω–∞–π–¥–µ–Ω–∞")

local foundItems = 0

for _, id in ipairs(clothingIDs) do
    print("\nüîç –ü—Ä–æ–≤–µ—Ä—è–µ–º ID:", id)
    local template = "rbxassetid://"..id
    local itemsChecked = 0
    
    for _, item in ipairs(clothingFolder:GetDescendants()) do
        itemsChecked = itemsChecked + 1
        
        if item:IsA("Shirt") then
            if item.ShirtTemplate == template then
                print("‚úî –ù–∞–π–¥–µ–Ω–∞ —Ä—É–±–∞—à–∫–∞:", item:GetFullName())
                createMarker(item, "SHIRT: "..id, Color3.new(1, 0, 0))
                foundItems = foundItems + 1
            end
        elseif item:IsA("Pants") then
            if item.PantsTemplate == template then
                print("‚úî –ù–∞–π–¥–µ–Ω—ã —à—Ç–∞–Ω—ã:", item:GetFullName())
                createMarker(item, "PANTS: "..id, Color3.new(0, 1, 0))
                foundItems = foundItems + 1
            end
        end
    end
    
    print("–ü—Ä–æ–≤–µ—Ä–µ–Ω–æ –æ–±—ä–µ–∫—Ç–æ–≤ –¥–ª—è ID "..id..":", itemsChecked)
end

print("\n=== –ò–¢–û–ì–ò ===")
print("–í—Å–µ–≥–æ –ø—Ä–æ–≤–µ—Ä–µ–Ω–æ ID:", #clothingIDs)
print("–ù–∞–π–¥–µ–Ω–æ —Å–æ–≤–ø–∞–¥–µ–Ω–∏–π:", foundItems)
if foundItems == 0 then
    warn("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω–æ –Ω–∏ –æ–¥–Ω–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–∞ –æ–¥–µ–∂–¥—ã!")
    print("–í–æ–∑–º–æ–∂–Ω—ã–µ –ø—Ä–∏—á–∏–Ω—ã:")
    print("- –£–∫–∞–∑–∞–Ω—ã –Ω–µ–≤–µ—Ä–Ω—ã–µ ID")
    print("- –û–¥–µ–∂–¥–∞ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ –¥—Ä—É–≥–æ–π –ø–∞–ø–∫–µ")
    print("- –û–¥–µ–∂–¥–∞ –µ—â–µ –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω–∞")
end

function createMarker(item, text, color)
    local gui = Instance.new("BillboardGui")
    gui.Adornee = item
    gui.Size = UDim2.new(6, 0, 2, 0)
    gui.StudsOffset = Vector3.new(0, 2, 0)
    gui.Parent = item
    
    local label = Instance.new("TextLabel")
    label.Text = text
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.TextColor3 = color
    label.TextScaled = true
    label.Font = Enum.Font.SourceSansBold
    label.Parent = gui
end
