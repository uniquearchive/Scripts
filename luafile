-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è —Å–æ—Å—Ç–æ—è–Ω–∏–π
local NoclipEnabled = false
local PerformanceMode = false
local originalMaterials = {}
local foundPlayerItems = {}

local function createClothingMarker(item, info)
    if not item or not item.Parent then return end
    
    local adornee = item.Parent:FindFirstChild("Torso") or 
                   item.Parent:FindFirstChild("UpperTorso") or
                   item.Parent:FindFirstChild("HumanoidRootPart") or
                   item

    if not adornee then return end

    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    local itemName = info.name or info.type or "–ü—Ä–µ–¥–º–µ—Ç"
    local itemId = info.id or "N/A"
    local rarity = info.rarity or "–û–±—ã—á–Ω—ã–π"
    local itemType = info.type and info.type:lower() or "shirt"

    -- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ü–≤–µ—Ç —Ä–µ–¥–∫–æ—Å—Ç–∏
    local rarityColor
    if rarity == "Epic" then
        rarityColor = Color3.fromRGB(170, 0, 255) -- –§–∏–æ–ª–µ—Ç–æ–≤—ã–π
    elseif rarity == "Legendary" then
        rarityColor = Color3.fromRGB(255, 170, 0) -- –û—Ä–∞–Ω–∂–µ–≤—ã–π
    elseif rarity == "Ultimate" then
        rarityColor = Color3.fromRGB(255, 50, 50) -- –ö—Ä–∞—Å–Ω—ã–π
    elseif rarity == "Mythic" then
        rarityColor = Color3.fromRGB(255, 215, 0)
    else
        rarityColor = Color3.fromRGB(200, 200, 200) -- –°–µ—Ä—ã–π –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    end

    -- –°–æ–∑–¥–∞–µ–º BillboardGui
    local gui = Instance.new("BillboardGui")
    gui.Name = "ClothingMarker"
    gui.Adornee = adornee
    gui.Size = UDim2.new(8, 0, 3, 0) -- –£–≤–µ–ª–∏—á–∏–ª–∏ –≤—ã—Å–æ—Ç—É –¥–ª—è –Ω–æ–≤–æ–≥–æ —Ç–µ–∫—Å—Ç–∞
    gui.StudsOffset = Vector3.new(0, 3.5, 0)
    gui.AlwaysOnTop = true
    gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    gui.Parent = adornee

    -- –û—Å–Ω–æ–≤–Ω–æ–π –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    frame.BackgroundTransparency = 0.3
    frame.BorderSizePixel = 0
    frame.Parent = gui

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame

    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫ (–Ω–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞)
    local title = Instance.new("TextLabel")
    title.Text = itemName
    title.Size = UDim2.new(1, -10, 0.3, 0) -- –£–º–µ–Ω—å—à–∏–ª–∏ –≤—ã—Å–æ—Ç—É
    title.Position = UDim2.new(0, 5, 0, 5)
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextScaled = true
    title.Font = Enum.Font.GothamBold
    title.BackgroundTransparency = 1
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = frame

    -- ID –ø—Ä–µ–¥–º–µ—Ç–∞
    local idLabel = Instance.new("TextLabel")
    idLabel.Text = "ID: " .. itemId
    idLabel.Size = UDim2.new(1, -10, 0.2, 0)
    idLabel.Position = UDim2.new(0, 5, 0.3, 0)
    idLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
    idLabel.TextScaled = true
    idLabel.Font = Enum.Font.Gotham
    idLabel.BackgroundTransparency = 1
    idLabel.TextXAlignment = Enum.TextXAlignment.Left
    idLabel.Parent = frame

    -- –†–µ–¥–∫–æ—Å—Ç—å (–∫—Ä–∞—Å–∏–≤–æ –æ—Ñ–æ—Ä–º–ª–µ–Ω–Ω–∞—è)
    local rarityFrame = Instance.new("Frame")
    rarityFrame.Size = UDim2.new(1, -10, 0.2, 0)
    rarityFrame.Position = UDim2.new(0, 5, 0.5, 0)
    rarityFrame.BackgroundColor3 = rarityColor
    rarityFrame.BackgroundTransparency = 0.7
    rarityFrame.BorderSizePixel = 0
    rarityFrame.Parent = frame

    local rarityCorner = Instance.new("UICorner")
    rarityCorner.CornerRadius = UDim.new(0, 4)
    rarityCorner.Parent = rarityFrame

    local rarityLabel = Instance.new("TextLabel")
    rarityLabel.Text = rarity:upper()
    rarityLabel.Size = UDim2.new(1, 0, 1, 0)
    rarityLabel.Position = UDim2.new(0, 0, 0, 0)
    rarityLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    rarityLabel.TextScaled = true
    rarityLabel.Font = Enum.Font.GothamBold
    rarityLabel.BackgroundTransparency = 1
    rarityLabel.Parent = rarityFrame

    -- –ò–∫–æ–Ω–∫–∞ —Ç–∏–ø–∞
    local typeIcon = Instance.new("ImageLabel")
    typeIcon.Size = UDim2.new(0.1, 0, 0.3, 0)
    typeIcon.Position = UDim2.new(0.9, -5, 0.7, 0)
    typeIcon.BackgroundTransparency = 1
    typeIcon.Image = "rbxassetid://10734951510"
    typeIcon.Parent = frame

    return gui
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—á–∏—Å—Ç–∫–∏ –≤—Å–µ—Ö –º–∞—Ä–∫–µ—Ä–æ–≤
local function clearAllMarkers()
    for _, item in ipairs(workspace:GetDescendants()) do
        if item.Name == "ClothingMarker" then
            item:Destroy()
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è –ø—Ä–æ–≤–µ—Ä–∫–∏ —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏—è ID –≤ —Ç–∞–±–ª–∏—Ü–µ
local function isIdExists(id)
    for _, item in ipairs(clothingItems) do
        if tostring(item.id) == tostring(id) then
            return true
        end
    end
    return false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –≤ —Ç–∞–±–ª–∏—Ü—É
local function addToClothingTable(itemsToAdd)
    local addedCount = 0
    
    for _, newItem in ipairs(itemsToAdd) do
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ —É–∂–µ —Ç–∞–∫–æ–π ID
        if not isIdExists(newItem.id) then
            -- –°–æ–∑–¥–∞–µ–º –∑–∞–ø–∏—Å—å —Å –∑–Ω–∞—á–µ–Ω–∏—è–º–∏ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
            local itemToAdd = {
                id = newItem.id or 0,
                name = newItem.type or "Item", -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —Ç–∏–ø –∫–∞–∫ –∏–º—è –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
                type = newItem.type and newItem.type:lower() or "shirt",
                rarity = newItem.rarity or "Unknown"
            }
            
            table.insert(clothingItems, itemToAdd)
            addedCount = addedCount + 1
        end
    end
    
    return addedCount
end

-- –û–±–Ω–æ–≤–ª—ë–Ω–Ω—ã–π —Å–ø–∏—Å–æ–∫ —Ä–µ–¥–∫–æ—Å—Ç–µ–π (–±–µ–∑ Common)
local rarities = {"Epic", "Legendary", "Ultimate", "Mythic"}

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –≤—ã–±—Ä–∞–Ω–Ω–æ–π —Ä–µ–¥–∫–æ—Å—Ç–∏
local selectedRarity = "Epic"

-- Dropdown –¥–ª—è –≤—ã–±–æ—Ä–∞ —Ä–µ–¥–∫–æ—Å—Ç–∏
local rarityDropdown = Tabs.Main:AddDropdown("RarityDropdown", {
    Title = "–í—ã–±–æ—Ä —Ä–µ–¥–∫–æ—Å—Ç–∏",
    Description = "–í—ã–±–µ—Ä–∏—Ç–µ —Ä–µ–¥–∫–æ—Å—Ç—å –¥–ª—è —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–∏",
    Values = rarities,
    Multi = false,
    Default = selectedRarity
})

rarityDropdown:OnChanged(function(Value)
    selectedRarity = Value
end)

-- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ –æ–¥–µ–∂–¥—ã –ø–æ –±–∞–∑–µ —Å —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–µ–π
local function findClothingItemsFiltered(filterRarity)
    clearAllMarkers()

    local clothingFolder = workspace:FindFirstChild("Clothing")
    if not clothingFolder then
        Fluent:Notify({
            Title = "–û—à–∏–±–∫–∞",
            Content = "–ü–∞–ø–∫–∞ Clothing –Ω–µ –Ω–∞–π–¥–µ–Ω–∞!",
            Duration = 5
        })
        return
    end

    local foundCount = 0
    local searchStart = os.clock()

    for _, info in ipairs(clothingItems) do
        if not filterRarity or info.rarity == filterRarity then
            local partialId = string.sub(tostring(info.id), 1, 6)

            for _, item in ipairs(clothingFolder:GetDescendants()) do
                local itemTemplate = (info.type == "Shirt" and item:IsA("Shirt") and item.ShirtTemplate) or
                                     (info.type == "Pants" and item:IsA("Pants") and item.PantsTemplate)

                if itemTemplate then
                    local templateId = string.match(itemTemplate, "rbxassetid://(%d+)$")
                    if templateId and string.sub(templateId, 1, #partialId) == partialId then
                        createClothingMarker(item, info)
                        foundCount = foundCount + 1
                        break
                    end
                end
            end
        end
    end

    local searchTime = math.floor((os.clock() - searchStart) * 1000)

    Fluent:Notify({
        Title = "–ü–æ–∏—Å–∫ –∑–∞–≤–µ—Ä—à–µ–Ω",
        Content = filterRarity and
            string.format("–ù–∞–π–¥–µ–Ω–æ %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤ —Å —Ä–µ–¥–∫–æ—Å—Ç—å—é: %s", foundCount, filterRarity) or
            string.format("–ù–∞–π–¥–µ–Ω–æ –ø—Ä–µ–¥–º–µ—Ç–æ–≤: %d (–≤—Å–µ —Ä–µ–¥–∫–æ—Å—Ç–∏)", foundCount),
        SubContent = string.format("–í—Ä–µ–º—è –ø–æ–∏—Å–∫–∞: %d –º—Å", searchTime),
        Duration = 5
    })
end

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è –ø–æ–∏—Å–∫–∞ –ø–æ –≤—ã–±—Ä–∞–Ω–Ω–æ–π —Ä–µ–¥–∫–æ—Å—Ç–∏
Tabs.Main:AddButton({
    Title = "–ü–æ–∏—Å–∫ –ø–æ –≤—ã–±—Ä–∞–Ω–Ω–æ–π —Ä–µ–¥–∫–æ—Å—Ç–∏",
    Description = "–ò—â–µ—Ç –ø—Ä–µ–¥–º–µ—Ç—ã —Å –≤—ã–±—Ä–∞–Ω–Ω–æ–π —Ä–µ–¥–∫–æ—Å—Ç—å—é",
    Callback = function()
        findClothingItemsFiltered(selectedRarity)
    end
})

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è –ø–æ–∏—Å–∫–∞ –ø–æ –≤—Å–µ–º —Ä–µ–¥–∫–æ—Å—Ç—è–º
Tabs.Main:AddButton({
    Title = "–ü–æ–∏—Å–∫ –ø–æ –≤—Å–µ–º —Ä–µ–¥–∫–æ—Å—Ç—è–º",
    Description = "–ò—â–µ—Ç –ø—Ä–µ–¥–º–µ—Ç—ã –Ω–µ–∑–∞–≤–∏—Å–∏–º–æ –æ—Ç —Ä–µ–¥–∫–æ—Å—Ç–∏",
    Callback = function()
        findClothingItemsFiltered(nil)
    end
})

-- –ú–æ–¥–∏—Ñ–∏—Ü–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ —É –∏–≥—Ä–æ–∫–æ–≤
local function scanPlayersClothing()
    clearAllMarkers()
    local foundItems = {} -- –í—Ä–µ–º–µ–Ω–Ω–∞—è —Ç–∞–±–ª–∏—Ü–∞ –¥–ª—è –Ω–∞–π–¥–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
    
    local players = game:GetService("Players"):GetPlayers()
    local totalScanned = 0
    local rareItemsFound = 0
    
    for _, player in ipairs(players) do
        local clothingFolder = player:FindFirstChild("Clothing")
        if clothingFolder then
            for _, descendant in ipairs(clothingFolder:GetDescendants()) do
                if descendant:IsA("StringValue") and 
                   (descendant.Value == "Epic" or descendant.Value == "Legendary" or descendant.Value == "Ultimate" or descendant.Value == "Mythic") then
                    
                    local parent = descendant.Parent
                    local idValue = parent:FindFirstChild("id")
                    local typeValue = parent:FindFirstChild("Type")
                    
                    if idValue and idValue:IsA("IntValue") and typeValue and typeValue:IsA("StringValue") then
                        if typeValue.Value:lower() == "shirt" or typeValue.Value:lower() == "pants" then
                            local itemInfo = {
                                playerName = player.Name,
                                rarity = descendant.Value,
                                id = idValue.Value,
                                type = typeValue.Value,
                                object = parent
                            }
                            table.insert(foundItems, itemInfo)
                            rareItemsFound = rareItemsFound + 1
                        end
                    end
                end
            end
            totalScanned = totalScanned + 1
        end
    end
    
    -- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã
    if #foundItems > 0 then
        local message = string.format("–ù–∞–π–¥–µ–Ω–æ %d —Ä–µ–¥–∫–∏—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ —É %d –∏–≥—Ä–æ–∫–æ–≤:", rareItemsFound, totalScanned)
       -- for i, item in ipairs(foundItems) do
        --    message = message .. string.format("\n%d. %s: %s (ID: %d) - %s", 
       --         i, item.playerName, item.type, item.id, item.rarity)
      --  end
        
        Window:Dialog({
            Title = "–†–µ–∑—É–ª—å—Ç–∞—Ç—ã –ø–æ–∏—Å–∫–∞",
            Content = message,
            Buttons = {
                {
                    Title = "–î–æ–±–∞–≤–∏—Ç—å –≤ –±–∞–∑—É",
                    Callback = function()
                        local added = addToClothingTable(foundItems)
                        Fluent:Notify({
                            Title = "–ë–∞–∑–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∞",
                            Content = string.format("–î–æ–±–∞–≤–ª–µ–Ω–æ %d –Ω–æ–≤—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤", added),
                            SubContent = string.format("–ü—Ä–æ–ø—É—â–µ–Ω–æ %d –¥—É–±–ª–∏–∫–∞—Ç–æ–≤", #foundItems - added),
                            Duration = 5
                        })
                    end
                },
                {
                    Title = "–ü–æ–º–µ—Ç–∏—Ç—å –ø—Ä–µ–¥–º–µ—Ç—ã",
                    Callback = function()
                        for _, item in ipairs(foundItems) do
                            if item.object and item.object.Parent then
                                createClothingMarker(item.object, item)
                            end
                        end
                        Fluent:Notify({
                            Title = "–ì–æ—Ç–æ–≤–æ",
                            Content = "–ü—Ä–µ–¥–º–µ—Ç—ã –ø–æ–º–µ—á–µ–Ω—ã",
                            Duration = 3
                        })
                    end
                },
                {
            Title = "–ö–æ–ø–∏—Ä–æ–≤–∞—Ç—å —Å–ø–∏—Å–æ–∫",
            Callback = function()
                    -- –°–æ–∑–¥–∞–µ–º —Ç–∞–±–ª–∏—Ü—É –¥–ª—è —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö ID
                    local uniqueIds = {}
                    local filteredItems = {}
                    
                    -- –§–∏–ª—å—Ç—Ä—É–µ–º –ø—Ä–µ–¥–º–µ—Ç—ã, –∏—Å–∫–ª—é—á–∞—è –¥—É–±–ª–∏–∫–∞—Ç—ã –∏ —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏–µ –≤ clothingItems
                    for _, item in ipairs(foundItems) do
                        local idStr = tostring(item.id)
                        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ ID —É–Ω–∏–∫–∞–ª–µ–Ω –≤ –Ω–∞–π–¥–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–∞—Ö –∏ –æ—Ç—Å—É—Ç—Å—Ç–≤—É–µ—Ç –≤ –æ—Å–Ω–æ–≤–Ω–æ–π —Ç–∞–±–ª–∏—Ü–µ
                        if not uniqueIds[idStr] and not isIdExists(item.id) then
                            uniqueIds[idStr] = true
                            table.insert(filteredItems, item)
                        end
                    end
                    
                    -- –§–æ—Ä–º–∏—Ä—É–µ–º —Ç–µ–∫—Å—Ç –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è
                    local copyText
                    if #filteredItems > 0 then
                        copyText = string.format("–ù–∞–π–¥–µ–Ω–æ %d —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ (–æ—Ç—Ñ–∏–ª—å—Ç—Ä–æ–≤–∞–Ω–æ –∏–∑ %d):\n", #filteredItems, #foundItems)
                        for i, item in ipairs(filteredItems) do
                            copyText = copyText .. string.format("%d. %s: %s (ID: %d) - %s\n", 
                                i, item.playerName, item.type, item.id, item.rarity)
                        end
                    else
                        copyText = "–ù–µ—Ç –Ω–æ–≤—ã—Ö —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è"
                    end
                    
                    setclipboard(copyText)
                    Fluent:Notify({
                        Title = "–°–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ",
                        Content = #filteredItems > 0 and 
                                 string.format("–°–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ %d —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤", #filteredItems) or
                                 "–ù–µ—Ç –Ω–æ–≤—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è",
                        Duration = 3
                    })
                end
                },
                {
                    Title = "–ó–∞–∫—Ä—ã—Ç—å",
                    Callback = function() end
                }
            }
        })
    else
        Fluent:Notify({
            Title = "–ü–æ–∏—Å–∫ –∑–∞–≤–µ—Ä—à–µ–Ω",
            Content = string.format("–ü—Ä–æ–≤–µ—Ä–µ–Ω–æ %d –∏–≥—Ä–æ–∫–æ–≤. –†–µ–¥–∫–∏—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ.", totalScanned),
            Duration = 5
        })
    end
end


-- –§—É–Ω–∫—Ü–∏–∏ –¥–ª—è —Ä–µ–∂–∏–º–æ–≤
local function toggleNoclip(value)
    NoclipEnabled = value
    local character = game.Players.LocalPlayer.Character
    if character then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = not NoclipEnabled
            end
        end
    end
end

local function togglePerformanceMode(value)
    PerformanceMode = value
    
    if PerformanceMode then
        for _, part in ipairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") and not part:IsA("Terrain") then
                originalMaterials[part] = part.Material
                part.Material = Enum.Material.SmoothPlastic
            elseif part:IsA("UnionOperation") then
                originalMaterials[part] = part.Material
                part.Material = Enum.Material.SmoothPlastic
            end
        end
    else
        for part, material in pairs(originalMaterials) do
            if part and part.Parent then
                part.Material = material
            end
        end
        originalMaterials = {}
    end
end

Tabs.Main:AddButton({
    Title = "–û—á–∏—Å—Ç–∏—Ç—å –º–∞—Ä–∫–µ—Ä—ã",
    Description = "–£–¥–∞–ª—è–µ—Ç –≤—Å–µ —Å–æ–∑–¥–∞–Ω–Ω—ã–µ –º–∞—Ä–∫–µ—Ä—ã –∏ –ø–æ–¥—Å–≤–µ—Ç–∫–∏",
    Callback = clearAllMarkers
})

-- –ö–Ω–æ–ø–∫–∞ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –±–∞–∑—ã (–∏—Å–ø—Ä–∞–≤–ª–µ–Ω–Ω–∞—è)
Tabs.Main:AddButton({
    Title = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –±–∞–∑—ã",
    Description = "–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Å–∫–æ–ª—å–∫–æ –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –∫–∞–∂–¥–æ–π –∫–∞—Ç–µ–≥–æ—Ä–∏–∏",
    Callback = function()
        local rarityCount = {}
        local typeCount = {Shirt = 0, Pants = 0}

        for _, item in ipairs(clothingItems) do
            -- –ü–æ–¥—Å—á—ë—Ç –ø–æ —Ä–µ–¥–∫–æ—Å—Ç–∏
            rarityCount[item.rarity] = (rarityCount[item.rarity] or 0) + 1

            -- –ü–æ–¥—Å—á—ë—Ç –ø–æ —Ç–∏–ø—É
            if item.type == "Shirt" or item.type == "Pants" then
                typeCount[item.type] = typeCount[item.type] + 1
            end
        end

        -- –§–æ—Ä–º–∏—Ä—É–µ–º —Ç–µ–∫—Å—Ç
        local message = "üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –±–∞–∑—ã –ø—Ä–µ–¥–º–µ—Ç–æ–≤:\n"
        message = message .. string.format("–†—É–±–∞—à–µ–∫: %d\n", typeCount.Shirt)
        message = message .. string.format("–®—Ç–∞–Ω–æ–≤: %d\n", typeCount.Pants)
        message = message .. "\n–†–µ–¥–∫–æ—Å—Ç–∏:\n"

        for rarity, count in pairs(rarityCount) do
            message = message .. string.format("- %s: %d\n", rarity, count)
        end

        Window:Dialog({
            Title = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –±–∞–∑—ã",
            Content = message,
            Buttons = {
                {
                    Title = "–û–∫",
                    Callback = function() end
                }
            }
        })
    end
})


Tabs.Main:AddButton({
    Title = "–û—á–∏—Å—Ç–∏—Ç—å –¥—É–±–ª–∏–∫–∞—Ç—ã",
    Description = "–£–¥–∞–ª—è–µ—Ç –ø–æ–≤—Ç–æ—Ä—è—é—â–∏–µ—Å—è ID –∏–∑ –±–∞–∑—ã –æ–¥–µ–∂–¥—ã",
    Callback = function()
        local seenIds = {}
        local newTable = {}
        local removed = 0

        for _, item in ipairs(clothingItems) do
            if not seenIds[item.id] then
                seenIds[item.id] = true
                table.insert(newTable, item)
            else
                removed = removed + 1
            end
        end

        clothingItems = newTable

        Fluent:Notify({
            Title = "–î—É–±–ª–∏–∫–∞—Ç—ã —É–¥–∞–ª–µ–Ω—ã",
            Content = string.format("–£–¥–∞–ª–µ–Ω–æ: %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤", removed),
            Duration = 4
        })
    end
})

Tabs.Players:AddButton({
    Title = "–°–∫–∞–Ω–∏—Ä–æ–≤–∞—Ç—å –∏–≥—Ä–æ–∫–æ–≤",
    Description = "–ü–æ–∏—Å–∫ —Ä–µ–¥–∫–æ–π –æ–¥–µ–∂–¥—ã —É –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤",
    Callback = scanPlayersClothing
})

Tabs.Game:AddButton({
    Title = "–£–¥–∞–ª–∏—Ç—å –¥–≤–µ—Ä–∏",
    Description = "–£–¥–∞–ª—è–µ—Ç –≤—Å–µ –¥–≤–µ—Ä–∏",
    Callback = function()
        local doorsFolder = workspace:FindFirstChild("MainTimerForNew")
        if doorsFolder and doorsFolder:FindFirstChild("Doors") then
            doorsFolder.Doors:Destroy()
            Fluent:Notify({
                Title = "–ì–æ—Ç–æ–≤–æ",
                Content = "–î–≤–µ—Ä–∏ –±—ã–ª–∏ —É–¥–∞–ª–µ–Ω—ã.",
                Duration = 3
            })
        else
            Fluent:Notify({
                Title = "–û—à–∏–±–∫–∞",
                Content = "–ü–∞–ø–∫–∞ Doors –Ω–µ –Ω–∞–π–¥–µ–Ω–∞!",
                Duration = 3
            })
        end
    end
})

Tabs.Game:AddButton({
    Title = "–£–¥–∞–ª–∏—Ç—å —Ç–µ–ª–µ–ø–æ—Ä—Ç",
    Description = "–£–¥–∞–ª—è–µ—Ç tp-–ø–∞—Ä—Ç",
    Callback = function()
        local tpPart = workspace:FindFirstChild("MainTimerForNew")
        if tpPart and tpPart:FindFirstChild("tp") then
            tpPart.tp:Destroy()
            Fluent:Notify({
                Title = "–£–¥–∞–ª–µ–Ω–æ",
                Content = "–¢–µ–ª–µ–ø–æ—Ä—Ç –ø–∞—Ä—Ç –±—ã–ª —É–¥–∞–ª—ë–Ω.",
                Duration = 3
            })
        else
            Fluent:Notify({
                Title = "–û—à–∏–±–∫–∞",
                Content = "tp –Ω–µ –Ω–∞–π–¥–µ–Ω!",
                Duration = 3
            })
        end
    end
})


Tabs.Game:AddToggle("NoclipToggle", {
    Title = "–†–µ–∂–∏–º Noclip",
    Description = "–ü–æ–∑–≤–æ–ª—è–µ—Ç –ø—Ä–æ—Ö–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã",
    Default = false,
    Callback = toggleNoclip
})

Tabs.Game:AddToggle("PerfModeToggle", {
    Title = "–†–µ–∂–∏–º –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏",
    Description = "–ó–∞–º–µ–Ω—è–µ—Ç –º–∞—Ç–µ—Ä–∏–∞–ª—ã –Ω–∞ SmoothPlastic",
    Default = false,
    Callback = togglePerformanceMode
})

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
Window:SelectTab(1)
Fluent:Notify({
    Title = "Clothing Finder –∑–∞–≥—Ä—É–∂–µ–Ω",
    Content = string.format("–ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö: %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤", #clothingItems),
    Duration = 5
})
